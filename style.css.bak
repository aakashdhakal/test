}

.left .content .social a span {
	height: 40px;
	width: 40px;
	background: white;
	line-height: 40px;
	text-align: center;
	font-size: 18px;
	border-radius: 50%;
	transition: 0.3s;
	color: black;
}

.left .content .social a span:hover {
	opacity: 0.8;
}

.center .content .fas {
