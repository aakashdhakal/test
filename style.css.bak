#about {
	padding: 0.5rem 2rem;
	text-align: center;
	cursor: pointer;
	font-size: 20px;
	border-radius: 10px;
	border: none;
	background-color: #035282;
	color: #ffffff;
	font-family: Kanit;
	transition: all 0.2s ease;
}

#about:hover {
	background-color: #023554;
}
.custom-shape-divider-bottom-1646199425 {
    position: absolute;
    bottom: -20vh;
    left: 0;
    width: 100%;
    overflow: hidden;
    line-height: 0;
    transform: rotate(180deg);
}

.custom-shape-divider-bottom-1646199425 svg {
    position: relative;
    display: block;
    width: calc(100% + 10.3px);
    height: 66px;
}

.custom-shape-divider-bottom-1646199425 .shape-fill {
    fill: #ffffff;
}
.projects {
	height: 140vh;
	text-align: center;
}

.heading {
	font-size: 40px;
	font-family: Audiowide;
	margin-top: 5vh;
	color: black;
	margin-bottom: 5%;
	text-align: center;
}
.projects .heading hr {
	width: 15%;
	margin-left: auto;
	margin-right: auto;
	margin-top: 1%;
	border-top: 5px solid black;
}

.container {
	position: relative;
	z-index: 1;
	display: flex;
	justify-content: center;
	align-items: center;
	flex-wrap: wrap;
	margin: 40px 0;
	cursor: pointer;
}

.container .card {
	position: relative;
	width: 280px;
	height: 400px;
	background: rgba(255, 255, 255, 0.05);
	margin: 20px;
	box-shadow: 0 15px 35px rgba(0, 0, 0, 0.5);
	border-radius: 15px;
	display: flex;
	justify-content: center;
	align-items: center;
	backdrop-filter: blur(10px);
	overflow: hidden;
}

.container .card .content {
	position: relative;
	display: flex;
	justify-content: center;
	align-items: center;
	flex-direction: column;
	transition: 0.5s;
}

.container .card:hover .content {
	transform: translateY(-20px);
}

.container .card .content .imgBx {
	position: relative;
	width: 150px;
	height: 200px;
	overflow: hidden;
	transform: translateY(45%);
	transition: all 0.5s;
}

.container .card:hover .content .imgBx {
	transform: translateY(0);
}

.container .card .content .imgBx img {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	object-fit: cover;
}

.container .card .content .contentBx h3 {
	color: black;
	text-transform: uppercase;
	letter-spacing: 2px;
	font-weight: 500;
	font-size: 20px;
	text-align: center;
	line-height: 1.1em;
	transform: translateY(30px);
	opacity: 0;
	transition: 0.5s;
	margin-top: -20px;
	padding: 10px;
}

.container .card:hover .content .contentBx h3 {
	opacity: 1;
	transform: translateY(0px);
}

.container .card .content .contentBx h3 span {
	font-size: 15px;
	font-weight: 300;
	text-transform: initial;
}

.container .card .sci {
	position: absolute;
	bottom: 20px;
	display: flex;
	line-height: 2;
	margin-bottom: 0;
	vertical-align: middle;
}

.container .card .sci li {
	list-style: none;
	margin: 0 5px;
	transform: translateY(40px);
	transition: 0.5s;
	opacity: 0;
	cursor: default;
	font-style: normal;
	font-weight: 700;
	color: black;
	text-transform: uppercase;
	font-size: .6em;
	border: 1px solid black;
	border-radius: 2rem;
	padding: .2rem .85rem;
	position: relative;
	text-decoration: none;
}

.container .card:hover .sci li {
	transform: translateY(0px);
	opacity: 1;
}

.projects .card-container {
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.projects button {
	margin-top: 5%;
}
.blogs {
	height: 120vh;
	background-size: cover !important;
	background: url('.svg') no-repeat;
	text-align: center;
	margin-top: 10%;
}

.blogs .heading hr {
	width: 7%;
	margin-left: auto;
	margin-right: auto;
	border-top: 5px solid black;
	margin-top: 1%;
}

.card-list {
	display: flex;
	padding: 2.7rem;
	overflow-x: visible;
}

.card-1 {
	height: 350px;
	width: 250px;
	min-width: 250px;
	padding: 1.5rem;
	border-radius: 16px;
	background: rgba(255, 255, 255, 0.05);
	;
	box-shadow: -1rem 0 2rem #000;
	display: flex;
	flex-direction: column;
	transition: .2s;
	margin: 0;
	scroll-snap-align: start;
	clear: both;
	position: relative;
	cursor: pointer;
	backdrop-filter: blur(10px);
}

.card-1:hover {
	transform: translateY(-1rem);
}

.card-header {
	margin-bottom: auto;
}

.card-header p {
	font-size: 14px;
	margin: 0 0 1rem;
	color: #7a7a8c;
}

.card-header h2 {
	font-size: 20px;
	margin: .25rem 0 auto;
	text-decoration: none;
	color: inherit;
	border: 0;
	display: inline-block;
	cursor: pointer;
	margin-bottom: 20px;
}

.card-header h2:hover {
	background: linear-gradient(90deg, #ff8a00, #e52e71);
	text-shadow: none;
	-webkit-text-fill-color: transparent;
	-webkit-background-clip: text;
	background-clip: text;
}

.card-author {
	margin: 3rem 0 0;
	display: grid;
	grid-template-columns: 75px 1fr;
	align-items: center;
	position: relative;
}

.author-avatar {
	grid-area: auto;
	align-self: start;
	position: relative;
	box-sizing: border-box;
}

.author-avatar img {
	width: 40px;
	height: 40px;
	border-radius: 50%;
	display: block;
	overflow: hidden;
	margin: 16px 10px;
}

.author-name {
	grid-area: auto;
	box-sizing: border-box;
}

.author-name-prefix {
	font-style: normal;
	font-weight: 700;
	color: #7a7a8c;
}

.half-circle {
	position: absolute;
	bottom: 0;
	left: 0;
	width: 60px;
	height: 48px;
	fill: none;
	stroke: #ff8a00;
	stroke-width: 8;
	stroke-linecap: round;
	pointer-events: none;
}

.tags {
	margin: 1rem 0 2rem;
	padding: .5rem 0 1rem;
	line-height: 2;
	margin-bottom: 0;
	vertical-align: middle;
}

.tags a {
	font-style: normal;
	font-weight: 700;
	color: black;
	text-transform: uppercase;
	font-size: .6em;
	border: 1px solid black;
	border-radius: 2rem;
	padding: .5rem .85rem;
	position: relative;
	text-decoration: none;
}

footer svg {
	position: absolute;
	z-index: -1;
	width: 100%;
	margin-top: -95vh;
}

footer {
	position: relative;
	bottom: 0px;
	width: 100%;
}

.main-content {
	display: flex;
}

.main-content .box {
	flex-basis: 50%;
	padding: 10px 20px;
}

.box h2 {
	font-size: 1.125rem;
	font-weight: 600;
	text-transform: uppercase;
	color: var(--font-color);
	font-family: Audiowide;
}

.box .content {
	margin: 20px 0 0 0;
	position: relative;
}

.box .content:before {
	position: absolute;
	content: '';
	top: -10px;
	height: 2px;
	width: 100%;
	background: var(--font-color);
}

.box .content:after {
	position: absolute;
	content: '';
	height: 2px;
	width: 15%;
	background: #004684;
	top: -10px;
}

.left .content p {
	text-align: justify;
	color: var(--font-color);
	line-height: 1.5;
	font-family: Monda;
	font-weight: 600;
}

.left .content .social {
