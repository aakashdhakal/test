.welcome-container {
	line-height: 13vh;
	margin-top: 10%;
	margin-left: 5%;
	color: white;
	margin-top: 5%;
}

.welcome .image-container {
	margin: 10%;
	box-sizing: border-box;
	position: relative;
	right: -15%;
	bottom: 15%;
}

.welcome .image-container::before {
	content: "";
	position: absolute;
	width: 100%;
	height: 115%;
	border: 0.25rem solid white;
	top: 2rem;
	right: -2rem;
	border-radius: 5%;
}

.welcome .image-container img {
	max-width: 20rem;
	max-height: 25rem;
	-o-object-fit: cover;
	object-fit: cover;
	border-radius: 5%;
	position: relative;
	cursor: pointer;
	transition: all 0.2s ease-in;
}

.welcome .image-container button {
	position: absolute;
	margin: 10%;
	width: 18.7rem;
	margin-left: -100%;
	margin-top: 118%;
	transition: all 0.2s ease;
}
.welcome .social-icons
{
	list-style: none;
	display: flex;
	align-items: left;
	margin-left: -8%;
}
.welcome .social-icons a
{
	text-decoration: none;
	color: white;
	font-size: 20px;
}
.welcome .social-icons li 
{
	margin-left: 8%;
